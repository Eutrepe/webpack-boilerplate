{
  "name": "webpack-boilerplate",
  "version": "1.0.3",
  "description": "Boilerplate for webpack projects",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch webpack.config.ts --exec \"AUTOPREFIXER_GRID=autoplace webpack serve --mode=development --env dev\"",
    "build:code": "AUTOPREFIXER_GRID=autoplace webpack --mode=production --progress --env prod",
    "build:stats": "AUTOPREFIXER_GRID=autoplace webpack --mode=production --progress --json > stats.json --env prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build:analyzer": "webpack-bundle-analyzer --port 4200 stats.json",
    "build:full": "concurrently 'npm:build:code' 'npm:build:stats'",
    "build:en": "AUTOPREFIXER_GRID=autoplace webpack --mode=production --progress --env prod lang=en",
    "build:pl": "AUTOPREFIXER_GRID=autoplace webpack --mode=production --progress --env prod lang=pl",
    "build:after": "cp src/index.html public/",
    "build": "concurrently 'npm:build:pl' 'npm:build:en' && npm run build:after",
    "po2csv": "concurrently 'npm:po2csv:en' 'npm:po2csv:pl'",
    "po2csv:en": "node scripts/po2csv.js i18n/en.po > i18n/csv/en.csv",
    "po2csv:pl": "node scripts/po2csv.js i18n/pl.po > i18n/csv/pl.csv",
    "csv2po": "concurrently 'npm:csv2po:en' 'npm:csv2po:pl'",
    "csv2po:en": "node scripts/po2csv.js i18n/pot/en.pot i18n/csv/en.csv",
    "csv2po:pl": "node scripts/po2csv.js i18n/pot/pl.pot i18n/csv/pl.csv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eutrepe/webpack-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "author": "Maciej Bobrowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eutrepe/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/Eutrepe/webpack-boilerplate#readme",
  "engines": {
    "node": ">= 12.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@fast-csv/format": "^4.3.5",
    "@fast-csv/parse": "^4.3.6",
    "@messageformat/core": "^3.0.1",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.6",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.1.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "extract-loader": "^5.1.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "gettext-to-messageformat": "^0.4.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-loader": "^3.1.0",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "imagemin-gifsicle": "^7.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "pofile": "^1.1.3",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "precss": "^4.0.0",
    "prettier": "^2.6.2",
    "pug": "^3.0.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "simple-pug-loader": "^0.2.1",
    "style-loader": "^3.3.1",
    "svgo": "^2.8.0",
    "svgo-loader": "^3.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "core-js": "^3.21.1",
    "element-closest-polyfill": "^1.0.5",
    "intersection-observer": "^0.12.0",
    "regenerator-runtime": "^0.13.9",
    "whatwg-fetch": "^3.6.2"
  }
}
